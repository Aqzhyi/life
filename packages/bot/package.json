{
  "name": "bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run build:src",
    "dev": "concurrently --names 'ts,bot' 'npm run build:src -- --watch' 'bottender dev'",
    "build": "npm run build:src",
    "build:src": "TS_NODE_PROJECT=\"tsconfig-webpack.json\" webpack",
    "lint": "eslint .",
    "start": "bottender start",
    "test": "jest"
  },
  "dependencies": {
    "bottender": "^1.1.0",
    "dayjs": "^1.8.18",
    "debug": "^4.1.1",
    "delay": "^4.3.0",
    "immer": "^5.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.22",
    "@types/webpack": "^4.41.0",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
