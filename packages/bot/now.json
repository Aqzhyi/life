{
  "name": "warcraft3r",
  "version": 2,
  "builds": [
    {
      "src": "server/bot-server.ts",
      "use": "@now/node",
      "config": {
        "includeFiles": ["bottender.config.js", "dist/index.bundle.jsx"],
        "bundle": true
      }
    },
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/webhooks/.*",
      "dest": "server/bot-server.ts"
    }
  ],
  "env": {
    "LINE_ACCESS_TOKEN": "@line_access_token",
    "LINE_CHANNEL_SECRET": "@line_channel_secret",
    "LINE_ACCESS_TOKEN_BETA": "@line_access_token_beta",
    "LINE_CHANNEL_SECRET_BETA": "@line_channel_secret_beta",
    "BNET_CLIENT_ID": "@bnet_client_id",
    "BNET_CLIENT_SECRET": "@bnet_client_secret",
    "TWITCH_CLIENT_ID": "@twitch_client_id",
    "GOOGLE_ANALYTICS_UA_ID": "@google_analytics_ua_id",
    "GOOGLE_ACCESS_TOKEN": "@google_access_token",
    "GOOGLE_APPLICATION_CREDENTIALS": "@google_application_credentials",
    "GOOGLE_CLIENT_ID": "@google_client_id",
    "GOOGLE_CLIENT_SECRET": "@google_client_secret",
    "GOOGLE_OWN_CALENDAR_ID": "@google_own_calendar_id",
    "GOOGLE_REFRESH_TOKEN": "@google_refresh_token",
    "DEBUG": "w3r*,bottender:action*",
    "DEBUG_I18N": "0",
    "DEBUG_PRIVATE_KEEP": "bottender*,-messaging-api*,-bottender:session*-bottender:response"
  }
}
